{
    "name": "devixar/sleepingowl-file-manager",
    "type": "library",
    "description": "Use file manager inside SleepingOwl.",
    "keywords": [
        "Devixar",
        "SleepingowlFileManager",
        "Sleepingowl",
        "Admin",
        "FileManager",
        "Manager"
    ],
    "homepage": "https://github.com/Devixar/SleepingowlFileManager",
    "license": "MIT",
    "authors": [
        {
            "name": "Cyber-host",
            "email": "cyber@devixar.com",
            "homepage": "http://devixar.com/",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : "~5.5|~7.0",
        "laravelrus/sleepingowl": "^4.52",
        "jasekz/laradrop": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Devixar\\SleepingowlFileManager\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Devixar\\SleepingowlFileManager\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "format": "phpcbf --standard=psr2 src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
